    <%- contents %>

<footer class="footer">
  <div class="container">
    <div class="row">
      <div class="col-12">
        <div class="copyright d-flex flex-wrap align-items-center">
          <h2 class="me-3">EXHIBINECTION</h2>
          <h3>© 2020. All Rights Reserved.</h3>
          <ul class="submenu d-md-flex list-unstyled ms-md-auto d-none flex-wrap align-items-center">
            <li class="me-0 me-md-3"><a class="text-decoration-none"  href="#">Sign in</a></li>
            <li><a class="text-decoration-none" href="#">Sign up</a></li>
          </ul>
        </div>
      </div>
    </div>
  </div>
</footer>
  <!-- Swiper JS -->
  <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>

  <!-- Initialize Swiper -->
  <script>
    var swiper = new Swiper(".mySwiper", {
      slidesPerView: 3,
      spaceBetween: 30,
      freeMode: true,
      pagination: {
        el: ".swiper-pagination",
        clickable: true,
      },
    });
  </script>
    <script src="./assets/js/vendors.js"></script>
    <script src="./assets/js/all.js"></script>
  </body>

</html>