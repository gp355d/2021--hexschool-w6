  <body>
    <nav class="navbar navbar-expand-lg navbar-light">
      <div class="container position-relative">
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#menuList">
          <span class="navbar-toggler-icon"></span>
        </button>
        <a class="navbar-brand position-lg-absolute top-50 start-50 translate-lg-middle me-0" href="./index.html">
          <h1 class="fs-4 mb-0 text-center text-primary fw-normal">EXHIBINECTION</h1>
        </a>
        <a data-bs-toggle="offcanvas" data-bs-target="#offcanvasRight" class="nav-link px-3 d-flex align-items-center position-relative order-lg-3" href="javascript:;" data-bs-toggle="offcanvas" data-bs-target="#carts">
          <i class="material-icons">confirmation_number</i>
          <div class="cartNumber py-1 px-2 rounded-pill fs-10 bg-primary-light text-white position-absolute lh-1">3
          </div>
        </a>
        <div class="collapse navbar-collapse" id="menuList">
          <ul class="menu navbar-nav ms-auto mb-2 mb-lg-0">
            <li class="nav-item">
              <a class="nav-link px-3" href="#">Sign in</a>
            </li>
            <li class="nav-item">
              <a class="nav-link px-3" href="#">Sign up</a>
            </li>
          </ul>
        </div>
      </div>
      <div class="offcanvas offcanvas-end bg-dark" tabindex="-1" id="offcanvasRight" aria-labelledby="offcanvasRightLabel">
        <div class="offcanvas-header">
          <h5 id="offcanvasRightLabel" class="text-white">Cart List</h5>
          <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
        </div>
        <div class="offcanvas-body">
          <div class="card mb-3" style="max-width: 540px;">
            <div class="row g-0">
              <div class="col-md-4">
                <img src="./assets/images/hot.jpg" alt="...">
              </div>
              <div class="col-md-8">
                <div class="card-body">
                  <h5 class="card-title">2020 台味設計</h5>
                  <p class="card-text">01 JUN — 31 OCT</p>
                  <p class="card-text">全票</p>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
